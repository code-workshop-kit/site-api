{
  "name": "site-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "dev": "docker exec -it site-api_app_1 sh -c \"export NODE_ENV=development && npx nodemon src/index.js\"",
    "prod": "docker exec -it site-api_app_1 sh -c \"export NODE_ENV=production && node src/index.js\"",
    "inspect": "docker exec -it site-api_psql_1 psql -U joren",
    "test": "docker exec -it site-api_app_1 npx mocha ./test/**/*.test.js --exit",
    "test:ci": "npx mocha ./test/**/*.test.js --exit",
    "test:watch": "docker exec -it site-api_app_1 npx mocha ./test/**/*.test.js --watch",
    "seed": "docker exec -it site-api_app_1 npx knex migrate:latest --env development && docker exec -it site-api_app_1 npx knex seed:run --env development"
  },
  "keywords": [],
  "author": "Joren Broekema",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "sinon": "^9.2.1"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "knex": "^0.21.12",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^10.0.0",
    "koa-session": "^6.1.0",
    "mailgun-js": "^0.22.0",
    "node-fetch": "^2.6.1",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "pg": "^8.4.2",
    "stripe": "^8.132.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "printWidth": 100
  }
}
